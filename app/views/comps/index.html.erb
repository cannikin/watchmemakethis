<%= render :partial => 'shared/title', :locals => { :title => 'Listing Comps' } %>

<table>
  <tr>
    <th>Title</th>
    <th>Project</th>
  </tr>

<% for comp in @comps %>
  <tr>
    <td><%=h comp.title %></td>
    <td><%=h comp.project_id %></td>
    <td><%= link_to 'Show', comp %></td>
    <td><%= link_to 'Edit', edit_comp_path(comp) %></td>
    <td><%= link_to 'Destroy', comp, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New comp', new_comp_path %>
